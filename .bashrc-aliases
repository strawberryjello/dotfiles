# git
alias gt='git status '
alias ga='git add '
alias gaa='git add .'
alias gc='git commit -m'
alias gl='git pull '
alias gh='git push '
alias gd='git diff --color'
alias gb='git branch '
alias go='git checkout '
alias gsa='git stash save'
alias gsh='git stash show'
alias gsl='git stash list'
alias gsp='git stash pop'
